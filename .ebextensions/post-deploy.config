commands:
  create_post_dir:
    command: "mkdir -p /opt/elasticbeanstalk/hooks/configdeploy/post"
    ignoreErrors: false
files:
  "/opt/elasticbeanstalk/hooks/configdeploy/post/set_env_chmod.sh":
    mode: "000755"
    owner: root
    group: root
    content: |
      cd /var/app/current/
      sudo touch testingshellscriptBUNNY.hi
      /opt/elasticbeanstalk/bin/get-config environment > /tmp/envs.json
      echo "After envs.json write"
      mv /tmp/envs.json /var/app/current
      echo "After envs.json move"
      cd cookies
      chmod a+w -R .