{
  "name": "eatifiy-crm",
  "version": "1.0.0",
  "description": "Internal Tools for Eatify",
  "main": "server/index.js",
  "scripts": {
    "test": "mocha",
    "bundle": "browserify client/ -t babelify -o server/public/main.js",
    "bundle:watch": "watchify client/ -vd -t babelify -o server/public/main.js",
    "start": "$(node -e \"var vars = JSON.parse('$(sudo /opt/elasticbeanstalk/bin/get-config environment)'); console.log(Object.keys(vars).map((key) => \\`\\${key}=\\\"\\${vars[key]}\"\\`).join(' '));\") node .",
    "start:watch": "nodemon --ignore cookies/*.json --ignore server/cookies/*.json .",
    "watch": "run-p bundle:watch start:watch",
    "migrate:make": "knex migrate:make",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jyaaan/eatifiy-crm.git"
  },
  "author": "John Yamashiro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jyaaan/eatifiy-crm/issues"
  },
  "homepage": "https://github.com/jyaaan/eatifiy-crm#readme",
  "dependencies": {
    "async": "^2.4.1",
    "aws-sdk": "^2.125.0",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "chromedriver": "^2.29.0",
    "express": "^4.15.3",
    "fast-csv": "^2.4.0",
    "instagram-private-api": "^0.5.16",
    "knex": "^0.13.0",
    "mocha": "^3.4.2",
    "node-uuid": "^1.4.8",
    "pg": "^6.2.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "redux": "^3.6.0",
    "request": "^2.81.0",
    "selenium-webdriver": "^3.4.0",
    "semantic-ui-react": "^0.70.0",
    "socket.io": "^2.0.3",
    "twilio": "^3.9.3"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^2.0.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "request": "^2.81.0",
    "watchify": "^3.9.0"
  }
}
